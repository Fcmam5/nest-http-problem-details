{
  "version": 2,
  "cli": {
    "defaultCollection": "@nrwl/nest"
  },
  "defaultProject": "example-with-default-settings",
  "projects": {
    "example-with-default-settings": {
      "root": "examples/example-with-default-settings",
      "sourceRoot": "examples/example-with-default-settings/src",
      "projectType": "application",
      "targets": {
        "build": {
          "executor": "@nrwl/node:build",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/examples/example-with-default-settings",
            "main": "examples/example-with-default-settings/src/main.ts",
            "tsConfig": "examples/example-with-default-settings/tsconfig.app.json"
          },
          "configurations": {
            "production": {
              "optimization": true,
              "extractLicenses": true,
              "inspect": false,
              "fileReplacements": [
                {
                  "replace": "examples/example-with-default-settings/src/environments/environment.ts",
                  "with": "examples/example-with-default-settings/src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "executor": "@nrwl/node:execute",
          "options": {
            "buildTarget": "example-with-default-settings:build"
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "examples/example-with-default-settings/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/examples/example-with-default-settings"
          ],
          "options": {
            "jestConfig": "examples/example-with-default-settings/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nest-problem-details-filter": {
      "root": "libs/nest-problem-details-filter",
      "sourceRoot": "libs/nest-problem-details-filter/src",
      "projectType": "library",
      "targets": {
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/nest-problem-details-filter",
            "tsConfig": "libs/nest-problem-details-filter/tsconfig.lib.json",
            "packageJson": "libs/nest-problem-details-filter/package.json",
            "main": "libs/nest-problem-details-filter/src/index.ts",
            "assets": [
              "libs/nest-problem-details-filter/*.md"
            ]
          }
        },
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/nest-problem-details-filter/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/nest-problem-details-filter"
          ],
          "options": {
            "jestConfig": "libs/nest-problem-details-filter/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
    "nest-better-http-exceptions": {
      "root": "libs/nest-better-http-exceptions",
      "sourceRoot": "libs/nest-better-http-exceptions/src",
      "projectType": "library",
      "targets": {
        "lint": {
          "executor": "@nrwl/linter:eslint",
          "options": {
            "lintFilePatterns": [
              "libs/nest-better-http-exceptions/**/*.ts"
            ]
          }
        },
        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/nest-better-http-exceptions"
          ],
          "options": {
            "jestConfig": "libs/nest-better-http-exceptions/jest.config.js",
            "passWithNoTests": true
          }
        },
        "build": {
          "executor": "@nrwl/node:package",
          "outputs": [
            "{options.outputPath}"
          ],
          "options": {
            "outputPath": "dist/libs/nest-better-http-exceptions",
            "tsConfig": "libs/nest-better-http-exceptions/tsconfig.lib.json",
            "packageJson": "libs/nest-better-http-exceptions/package.json",
            "main": "libs/nest-better-http-exceptions/src/index.ts",
            "assets": [
              "libs/nest-better-http-exceptions/*.md"
            ]
          }
        }
      }
    }
  }
}